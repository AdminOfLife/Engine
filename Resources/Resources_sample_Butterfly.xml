<resources>
  <!-- Models -->
  <res name="Cube">
    <path>simple_cube.obj</path>
  </res>
  <res name="butterfly" type="model">
    <path>butterfly.obj</path>
  </res>
  <res name="heightmap" type="model">
    <path>Heightmap.png</path>
  </res>
  <!-- Additional Textures -->
  <res name="grass_tex" type="image">
    <path>grass_tex.jpg</path>
  </res>
  <res name="rock_tex" type="image">
    <path>rock_tex.jpg</path>
  </res>
  <res name="crate_tex" type="image">
    <path>crate_tex.jpg</path>
  </res>
  <res name="skybox_tex_good" type="image">
    <path>skyBox_good.png</path>
  </res>
   <res name="skybox_tex_evil" type="image">
    <path>skyBox_evil.png</path>
  </res> 
    <res name="texturefont" type="image">
    <path>texturefont.png</path>
  </res>
  <res name="texture_motyl">
    <path>motyl.png</path>
  </res>
  <!-- Shaders -->
  <res name="shader_heightmap" type="shader">
    <frag>heightmap.frag</frag>
    <vert>heightmap.vert</vert>
  </res>
  <res name="shader_simple" type="shader">
    <frag>simple.frag</frag>
    <vert>simple.vert</vert>
  </res>
  <res name="shader_light" type="light">
    <frag>light.frag</frag>
    <vert>light.vert</vert>
  </res>
  <res name="shader_interface" type="shader">
    <frag>interface.frag</frag>
    <vert>interface.vert</vert>
  </res>
    <res name="shader_font" type="shader">
    <frag>font.frag</frag>
    <vert>font.vert</vert>
  </res>
  <res name="shader_texturing" type="shader">
    <frag>texturing.frag</frag>
    <vert>texturing.vert</vert>
  </res>
  <res name="shader_mirror">
    <frag>mirror.frag</frag>
    <vert>mirror.vert</vert>
  </res>
  
  <res name="cube_fbx" type="model">
    <!--path>simple_cube.fbx</path-->
    <path>../Resources/motylsingle.fbx</path>
  </res>
  <res name="ground">
    <HScale>50</HScale><!--X and Z scales, determines map size-->
    <WScale>3</WScale><!--Y scale, determines depths-->
  </res>
</resources>

